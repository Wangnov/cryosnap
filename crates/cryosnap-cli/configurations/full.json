    "level": 0,
    "strip": "safe",
    "quantize": false,
    "quantize_quality": 85,
    "quantize_speed": 4,
    "quantize_dither": 1
  },
  "title": {
    "enabled": true,
    "path_style": "absolute",
    "tmux_format": "#{session_name}:#{window_index}.#{pane_index} #{pane_title}",
    "align": "center",
    "size": 12,
    "color": "#C5C8C6",
    "opacity": 0.85,
    "max_width": 80,
