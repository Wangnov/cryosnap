{
  "window": true,
  "theme": "charm",
  "border": {
    "radius": 8,
    "width": 1,
    "color": "#515151"
  },
  "shadow": {
    "blur": 24,
    "x": 0,
    "y": 12
  },
  "padding": [
    20,
    40,
    20,
    20
  ],
  "margin": [
    50,
    60,
    70,
    60
  ],
  "background": "#171717",
  "font": {
    "family": "monospace",
    "size": 14,
    "ligatures": true,
    "fallbacks": [],
